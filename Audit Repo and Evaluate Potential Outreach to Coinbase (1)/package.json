{
  "name": "base-name-service-ens-fork",
  "version": "1.0.0",
  "description": "Base Name Service - Properly forked from ENS for Base L2",
  "main": "index.js",
  "scripts": {
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:gas": "REPORT_GAS=true hardhat test > gasReport.txt",
    "coverage": "hardhat coverage",
    "deploy:testnet": "hardhat run scripts/deploy-base-ens.js --network base-sepolia",
    "deploy:mainnet": "hardhat run scripts/deploy-base-ens.js --network base",
    "verify": "hardhat verify --network base",
    "verify:deployment": "hardhat run scripts/verify-deployment.js",
    "flatten": "hardhat flatten",
    "register": "hardhat run scripts/register-name.js",
    "lint": "npm run lint:sol && npm run lint:js",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "lint:js": "eslint --ext .js,.ts .",
    "lint:fix": "npm run lint:sol -- --fix && npm run lint:js -- --fix",
    "format": "prettier --write 'contracts/**/*.sol' 'scripts/**/*.js' 'test/**/*.js'",
    "clean": "hardhat clean",
    "node": "hardhat node",
    "multisig:transfer": "hardhat run scripts/transfer-to-multisig.js",
    "post-deploy": "hardhat run scripts/post-deploy-config.js"
  },
  "keywords": [
    "base",
    "ethereum",
    "ens",
    "domains",
    "web3",
    "naming",
    "L2"
  ],
  "author": "Base Name Service",
  "license": "MIT",
  "dependencies": {
    "@ensdomains/buffer": "^0.1.1",
    "@ensdomains/ens-contracts": "^0.0.22",
    "@openzeppelin/contracts": "^4.9.3",
    "dns-packet": "^5.6.1",
    "eth-ens-namehash": "^2.0.8"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^10.0.0",
    "@typechain/ethers-v6": "^0.4.0",
    "@typechain/hardhat": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "chai": "^4.3.10",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "ethers": "^6.8.0",
    "hardhat": "^2.19.0",
    "hardhat-gas-reporter": "^1.0.9",
    "prettier": "^3.0.0",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.6.2",
    "solidity-coverage": "^0.8.5",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
